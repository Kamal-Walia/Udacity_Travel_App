{
  "name": "udacity-travel-app",
  "version": "1.0.0",
  "description": "A travel app that obtains a desired trip location & date from the user, and displays weather and an image of the location using information obtained from external APIs.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node ./src/server/server.js",
    "build": "webpack",
    "build-dev": "webpack-dev-server --config webpack.dev.js --open",
    "build-prod": "webpack --config webpack.prod.js"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-jest": "^29.7.0",
    "babel-loader": "^8.3.0",
    "body-parser": "^1.20.1",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "cors": "^2.8.5",
    "css-loader": "^6.7.3",
    "express": "^4.19.2",
    "express-async-await": "^1.1.0",
    "express-async-errors": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "jquery": "^3.5.1",
    "node-fetch": "^3.2.10",
    "node-sass": "^9.0.0",
    "popper.js": "^1.15.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^1.3.0",
    "supertest": "^4.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    }
  }
}
